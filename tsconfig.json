{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "lib": ["ES2021", "DOM"]
  },
  "include": [
    ".astro/types.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.astro"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
